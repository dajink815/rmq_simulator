<?xml version="1.0" encoding="UTF-8"?>
<scenario name="a2s_inonly">

    <send class="Message"  target="T_AWF">
        <header class="header">
            <field name="type" type="str" value="INCOMING_CALL_REQ"/>
            <field name="transactionId" type="str" value="[tId]"/>
            <field name="msgFrom" type="str" value="[rmq_local]"/>
            <field name="reason" type="str" value="SUCCESS"/>
            <field name="reasonCode" type="int" value="0"/>
            <field name="timestamp" type="str" value="[timestamp]"/>
        </header>
        <body class="body">
            <field name="callId" type="str" value="[tId]"/>
        </body>
    </send>

    <recv class="Message">
        <body class="incoming_call_res"/>
    </recv>

    <send class="Message" target="T_AWF">
        <header class="header">
            <field name="type" type="str" value="CALL_START_REQ"/>
            <field name="transactionId" type="str" value="[tId]"/>
            <field name="msgFrom" type="str" value="[rmq_local]"/>
            <field name="reason" type="str" value="SUCCESS"/>
            <field name="reasonCode" type="int" value="0"/>
            <field name="timestamp" type="str" value="[timestamp]"/>
        </header>
        <body class="body">
            <field name="taskId" type="str" value="[last_taskId]"/>
            <field name="callId" type="str" value="[last_callId]"/>
        </body>
    </send>

    <recv class="Message">
        <body class="call_start_res"/>
    </recv>

    <pause milliseconds="5000"/>

    <send class="Message" target="T_AWF">
        <header class="header">
            <field name="type" type="str" value="CALL_STOP_REQ"/>
            <field name="transactionId" type="str" value="[tId]"/>
            <field name="msgFrom" type="str" value="[rmq_local]"/>
            <field name="reason" type="str" value="SUCCESS"/>
            <field name="reasonCode" type="int" value="0"/>
            <field name="timestamp" type="str" value="[timestamp]"/>
        </header>
        <body class="body">
            <field name="taskId" type="str" value="[last_taskId]"/>
            <field name="callId" type="str" value="[last_callId]"/>
        </body>
    </send>

    <recv class="Message">
        <body class="call_stop_res"/>
    </recv>

    <recv class="Message">
        <body class="task_end_req"/>
    </recv>

    <send class="Message" target="T_AWF">
        <header class="header">
            <field name="type" type="str" value="TASK_END_RES"/>
            <field name="transactionId" type="str" value="[last_transactionId]"/>
            <field name="msgFrom" type="str" value="[rmq_local]"/>
            <field name="reason" type="str" value="SUCCESS"/>
            <field name="reasonCode" type="int" value="0"/>
            <field name="timestamp" type="str" value="[timestamp]"/>
        </header>
        <body class="body">
            <field name="taskId" type="str" value="[last_taskId]"/>
            <field name="callId" type="str" value="[last_callId]"/>
        </body>
    </send>

    <pause milliseconds="4000"/>

</scenario>
